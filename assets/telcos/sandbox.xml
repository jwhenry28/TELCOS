<telco name="sandbox">
	<users>
		<user name="user" password="password" home="/home/user" />
	</users>
	<shell builtins="help;whoami;users;pwd;ls;cd;cat;auth" guest="help;whoami;users;auth"></shell>
	<filesys>
		<inode name="" type="dir">
			<inode name="home" type="dir">
				<inode name="user" type="dir" permissions="user:rwx">
					<inode name="readme.txt" type="file">
						Congratulations, you've read this file!
					</inode>
					<inode name=".trash" type="dir">
						<inode name="old.txt" type="file">This is an old file</inode>
					</inode>
				</inode>
			</inode>
		</inode>
	</filesys>
</telco>
